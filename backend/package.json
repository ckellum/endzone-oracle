{
  "name": "endzone-oracle-backend",
  "version": "1.0.0",
  "description": "Backend API for EndZone Oracle - Serves Kalshi odds to all users",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "deploy": "gcloud functions deploy kalshi-odds-api --runtime nodejs20 --trigger-http --allow-unauthenticated --entry-point=getOdds",
    "logs": "gcloud functions logs read kalshi-odds-api --limit=50",
    "test": "node test.js"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "node-fetch": "^2.7.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "author": "EndZone Oracle",
